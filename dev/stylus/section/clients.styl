// CLIENTS
.clientsList
  max-width 1600px
  margin 0 auto
  background #000
  padding 50px 10px 100px 10px
  p
    padding-bottom 50px
    text-align center
    color #FFF
    text-transform uppercase
    font-family karla-regular
    font-size 18px
.clientsListItem
  display inline-block
  width 17.5%
  margin-left 2.5%
  &:first-child,
  &:nth-child(5n +1)
    margin-left 0
  img
    width 100%
  @media (max-width: 768px)
    width 30%
    margin-left 4%
    &:first-child,
    &:nth-child(3n +1)
      margin-left 0